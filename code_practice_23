
ip = "geeksforgeeks"

my = {}
for i in ip:
    my[i] =0
    
for i in ip:
    my[i] +=1
    
print(my)

for i in my:
    if my[i] >1:
        print(i)
 
 
############################################################################################
 
 
ls = [0]*256
for i in ip:
    ls[ord(i)] +=1
    
    
print(ls)

cnt =0
for i in range(256):
    if ls[i] >1:
        cnt +=1
        s = chr(i)
        print(s)
        
print(cnt)


